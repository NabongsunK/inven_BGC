{
  "name": "inven BGC",
  "description": "인생을 행복하게",
  "version": "1.0",
  "manifest_version": 3,
	"background": {
    "service_worker": "background.js"
  },

  "icons": {
    "16": "./images/icons8-color-16.png",
    "32": "./images/icons8-color-32.png",
    "48": "./images/icons8-color-48.png",
    "128": "./images/icons8-color-100.png"
  },

  "action": {
    "default_icon": {
      "16": "./images/icons8-color-16.png",
      "32": "./images/icons8-color-32.png",
      "48": "./images/icons8-color-48.png",
      "128": "./images/icons8-color-100.png"
    }
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  },
  
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "options_page": "options.html"
 

  
}
